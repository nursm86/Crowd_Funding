<!DOCTYPE html>
<html>
<head>
	<title>Edit Campaign</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Edit Campaign</legend>
			<table>
				<tr>
					<td>Target Fund</td>
					<td><input type="text" name="target_fund"value="<%= target_fund %>" ></td>
				</tr>
				<tr>
					<td>Raised Fund</td>
					<td><input type="text" name="raisedfund" value="<%= raised_fund %>"></td>
				</tr>
				<tr>
					<td>Campaign Type</td>
					<td><input type="text" name="campaigntype" value="<%= ctype %>"></td>
				</tr>
				<tr>
					<td>Description</td>
					<td><input type="text" name="description" value="<%= description %>"></td>
				</tr>
				<tr>
					<td>Image</td>
					<td><input type="text" name="image" value="<%= image %>"></td>
				</tr>
				<tr>
					<td>Publish Date</td>
					<td><input type="text" name="publishDate" value="<%= publishDate %>"></td>
				</tr>
				<tr>
					<td>End Date</td>
					<td><input type="text" name="enddate" value="<%= endDate %>"></td>
				</tr>
				<tr>
					<td>Title</td>
					<td><input type="text" name="title" value="<%= title %>"></td>
				</tr>
				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Submit"></td>
				</tr>
			</table>
		</fieldset>
	</form>
</body>
</html>
