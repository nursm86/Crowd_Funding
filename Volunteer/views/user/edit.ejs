<!DOCTYPE html>
<html>
<head>
	<title>Edit campaign</title>
	<link rel="stylesheet" type="text/css" href="/abc/css/style.css">
</head>
<body>
	

<h1 style="green"> Edit campaigns </h1>


		<fieldset>
			<legend>Create campaigns</legend>
			<form method="POST">
			<table>
				<tr>
					<td>campaign id</td>
					<td><input type="number" name="id" value="<%= c_id %>" readonly="true"></td>
				</tr>
				<tr>
					<td>Target_fund</td>
					<td><input type="number" name="target_fund" value="<%= target_fund %>"></td>
				</tr>
				<tr>
					<td>Raised_fund</td>
					<td><input type="number" name="raised_fund" value="<%= raised_fund %>"></td>
				</tr>
				<tr>
					<td>ctype</td>
					<td><input type="text" name="ctype" value="<%= ctype %>"></td>
				</tr>
				<tr>
					<td>description</td>
					<td><input type="text" name="description" value="<%= description %>"></td>
				</tr>
				<tr>
					<td>Publish date</td>
					<td><input type="text" name="Publish_date" value="<%= publisedDate %>"></td>
				</tr>
				<tr>
					<td>endDate</td>
					<td><input type="text" name="endDate" value="<%= endDate %>"></td>
				</tr>
				<tr>
					<td>status</td>
					<td><input type="number" name="status" value="<%= status %>"></td>
				</tr>
				<tr>
					<td>Title </td>
					<td><input type="text" name="title" value="<%= title %>"></td>
				</tr>

				<tr>
					<td>Image </td>
					<td>  <input type="file" id="img" name="img" ></td>
				</tr>

				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Submit"></td>
				</tr>
			</table>
		</fieldset>
	</form>

